    "      \"Descripción: 0.100000 Act360 Lin\\n\",\n",
    "      \"Obtener valor: 0.1\\n\",\n",
    "      \"Obtener nuevo valor: 0.12\\n\",\n",
    "      \"\\n\",\n",
    "      \"Descripción: 0.100000 Act365 Com\\n\",\n",
    "      \"Obtener valor: 0.1\\n\",\n",
    "      \"Obtener nuevo valor: 0.12\\n\",\n",
    "      \"\\n\",\n",
    "      \"Descripción: 0.100000 Act365 Exp\\n\",\n",
    "      \"Obtener valor: 0.1\\n\",\n",
    "      \"Obtener nuevo valor: 0.12\\n\",\n",
    "      \"\\n\"\n",
    "     ]\n",
